{
  "bin": {
    "coffee-jshint": "./cli.js"
  },
  "bugs": {
    "url": "https://github.com/marviq/coffee-jshint/issues"
  },
  "dependencies": {
    "commander": "^5.0.0",
    "jshint": "^2.11.0",
    "semver": "^7.1.3",
    "underscore": "^1.9.2"
  },
  "description": "Checks CoffeeScript source for errors using JSHint",
  "devDependencies": {
    "coffeescript": "^2.5.1",
    "standard-version": "^8.0.1"
  },
  "engines": {
    "node": ">=0.8",
    "npm": ">=5"
  },
  "files": [
    "AUTHORS",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "cli.js",
    "lib-js"
  ],
  "homepage": "https://github.com/marviq/coffee-jshint#readme",
  "keywords": [
    "coffee-script",
    "coffeescript",
    "jshint"
  ],
  "license": "BSD-3-Clause",
  "name": "coffee-jshint",
  "peerDependencies": {
    "coffeescript": ">= 1.6 <3"
  },
  "private": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marviq/coffee-jshint.git"
  },
  "scripts": {
    "hotfix": "standard-version --message 'chore(hotfix): %s'",
    "prepublishOnly": "make",
    "refresh": "set -x; npm install && npm prune && npm dedupe",
    "release": "standard-version"
  },
  "version": "1.2.0"
}
